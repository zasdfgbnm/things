\begin_body

\begin_layout Standard
To sample 
\begin_inset Formula $N$
\end_inset

 numbers from a size 
\begin_inset Formula $M$
\end_inset

 pool with replacement, there are in total 
\begin_inset Formula $M^{N}$
\end_inset

 cases.
 If the sampling is without replacement, there are 
\begin_inset Formula $P(M,N)$
\end_inset

 cases, where 
\begin_inset Formula $P\left(M,N\right)$
\end_inset

, also known as 
\begin_inset Formula $n\mathrm{P}r$
\end_inset

, is defined as 
\begin_inset Formula 
\[
P\left(M,N\right)=\frac{M!}{\left(M-N\right)!}.
\]

\end_inset

So the probability of getting non-duplicating keys (the correct case) is
\begin_inset Formula 
\[
\Pr\left[\text{unique keys}\right]=\frac{P\left(M,N\right)}{M^{N}}
\]

\end_inset


\end_layout

\begin_layout Standard
According to Stirling's formula
\begin_inset Formula 
\[
\sqrt{2\pi}n^{n+\frac{1}{2}}e^{-n}\leq n!\leq en^{n+\frac{1}{2}}e^{-n}
\]

\end_inset

we then have
\begin_inset Formula 
\[
\frac{\sqrt{2\pi}}{e}\frac{M^{M+\frac{1}{2}}e^{-M}}{\left(M-N\right)^{\left(M-N\right)+\frac{1}{2}}e^{-\left(M-N\right)}}\leq P\left(M,N\right)\leq\frac{e}{\sqrt{2\pi}}\frac{M^{M+\frac{1}{2}}e^{-M}}{\left(M-N\right)^{\left(M-N\right)+\frac{1}{2}}e^{-\left(M-N\right)}}
\]

\end_inset

which simplify to
\begin_inset Formula 
\[
\frac{\sqrt{2\pi}}{e}\frac{M^{M+\frac{1}{2}}}{\left(M-N\right)^{\left(M-N\right)+\frac{1}{2}}e^{N}}\leq P\left(M,N\right)\leq\frac{e}{\sqrt{2\pi}}\frac{M^{M+\frac{1}{2}}}{\left(M-N\right)^{\left(M-N\right)+\frac{1}{2}}e^{N}}
\]

\end_inset

so
\begin_inset Formula 
\[
\frac{\sqrt{2\pi}}{e}\frac{M^{(M-N)+\frac{1}{2}}}{\left(M-N\right)^{\left(M-N\right)+\frac{1}{2}}e^{N}}\leq\Pr\left[\text{unique keys}\right]\leq\frac{e}{\sqrt{2\pi}}\frac{M^{(M-N)+\frac{1}{2}}}{\left(M-N\right)^{\left(M-N\right)+\frac{1}{2}}e^{N}}
\]

\end_inset

which simplify to
\begin_inset Formula 
\[
\frac{\sqrt{2\pi}}{e}\left(\frac{M}{M-N}\right)^{(M-N)+\frac{1}{2}}\cdot e^{-N}\leq\Pr\left[\text{unique keys}\right]\leq\frac{e}{\sqrt{2\pi}}\left(\frac{M}{M-N}\right)^{(M-N)+\frac{1}{2}}\cdot e^{-N}
\]

\end_inset

to guarantee the correctness of the randperm algorithm, we need the left
 hand size above some threshold
\begin_inset Formula 
\[
\frac{\sqrt{2\pi}}{e}\left(\frac{M}{M-N}\right)^{(M-N)+\frac{1}{2}}\cdot e^{-N}\geq q
\]

\end_inset

which simplify to
\begin_inset Formula 
\[
\left(\frac{M}{M-N}\right)^{(M-N)+\frac{1}{2}}\geq\frac{q}{\sqrt{2\pi}}\cdot e^{N+1}
\]

\end_inset

to 
\end_layout

\end_body
